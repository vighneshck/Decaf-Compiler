class Program
{
	int num;
 	void initialize ()
	{
		num = callout ("read");
	}
	int hanoi (int n)
	{
		int temp, res, i, fin;
		res = 1;
		temp = n + 1;
		if (n == 1)
		{
			return 1;
		}
		for i = 1, temp
		{
			res = res * 2
		}
		fin = res - 1;
		return fin;
	}
	int main()
 	{
 		initialize ();
 		int result;
 		result = hanoi (num);
 		callout ("printf", "%d", result);
 		return 0;
 	}
}